<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="benz" class="com.example.demo.bean_factory.Car">
        <property name="name" value="benzbenz"></property>
    </bean>

    <bean id="myInstantiationAwareBeanPostProcessor" class="com.example.demo.bean_factory.MyInstantiationAwareBeanPostProcessor">
    </bean>
</beans>